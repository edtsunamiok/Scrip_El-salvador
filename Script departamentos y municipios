USE master
GO
--VERIFICAMOS QUE LA BASE DE DATOS NO EXISTA, SI NO LA ELIMINAMOS
IF EXISTS(SELECT * FROM SYS.DATABASES WHERE NAME = 'DB_EL_SALVADOR')
	DROP DATABASE DB_EL_SALVADOR;
GO

--CREAR BASE DE DATOS DB_CUENTAS
CREATE DATABASE DB_EL_SALVADOR;
GO


USE DB_EL_SALVADOR;
GO

--CREANDO TABLAS DEPARTAMENTO
CREATE TABLE DEPARTAMENTOS(
ID_DEPT INT IDENTITY(1,1) NOT NULL,--ID DEL DEARTAMENTO
NOMBRE_DEP NVARCHAR(50),--NOMBRE DEL DEPARTAMENTO
PRIMARY KEY(ID_DEPT)
)

GO
