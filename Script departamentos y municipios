SELECT D.NOMBRE_DEP, M.NOMBRE_MUNICP FROM DEPARTAMENTOS AS D INNER JOIN MUNICIPIOS AS M ON D.ID_DEPT = M.ID_DEPT
WHERE D.NOMBRE_DEP = 'San Salvador'

select * from MUNICIPIOS where NOMBRE_MUNICP = 'Tonacatepeque'
