SELECT * FROM DEPARTAMENTOS

--CREANDO TABLA MUNICIPIOS
CREATE TABLE MUNICIPIOS(
	ID_MUNICIP INT IDENTITY(1,1) NOT NULL,--ID DEL MUNICIPIO
	NOMBRE_MUNICP NVARCHAR(100)NOT NULL,--NOMBRE DEL MUNICIPIO
	ID_DEPT INT NOT NULL,-- ID DEL DEPARTAMENTO QUE PERTENECE EL MUNICIPIO
	PRIMARY KEY(ID_MUNICIP),
	CONSTRAINT FK_DEPT_MUNICP FOREIGN KEY(ID_DEPT) REFERENCES DEPARTAMENTOS(ID_DEPT)  
)

GO

